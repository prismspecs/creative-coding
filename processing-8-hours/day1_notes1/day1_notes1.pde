// the size function creates a window
// of width, height in pixels
size(500, 500);

background(0, 0, 255);

// take away the outline
noStroke();

// fill works in Red,Green,Blue
// the minimum value is 0, the max is 255
fill(188, 3, 130);

// ellipse draws a circle at x,y, width, height
ellipse(100, 100, 150, 150);

fill(255, 255, 255);

// set the thickness of the outline
strokeWeight(4);

// set the color of the stroke
stroke(0, 0, 0);

// draw a rectangle...
rect(30, 100, 200, 100);

// triangle...
triangle(250, 250, 500, 500, 250, 500);

